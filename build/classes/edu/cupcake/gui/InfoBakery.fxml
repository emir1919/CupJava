<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.cupcake.controllers.InfoBakeryController">
   <children>
      <TableView fx:id="TableId" layoutX="111.0" layoutY="49.0" prefHeight="200.0" prefWidth="378.0">
        <columns>
          <TableColumn fx:id="nom" prefWidth="107.0" text="Nom" />
          <TableColumn fx:id="adresse" text="Adresse" />
            <TableColumn fx:id="tel" prefWidth="59.0" text="Tel" />
            <TableColumn fx:id="fax" prefWidth="64.0" text="Fax" />
            <TableColumn fx:id="email" prefWidth="67.0" text="Email" />
        </columns>
      </TableView>
      <Button fx:id="Add" layoutX="144.0" layoutY="280.0" mnemonicParsing="false" onAction="#AddBakery" style="-fx-background-color: green; -fx-background-radius: 20;" text="Ajouter" textFill="#f8f8f8">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="update" layoutX="252.0" layoutY="280.0" mnemonicParsing="false" onAction="#UpdateBakery" style="-fx-background-color: blue; -fx-background-radius: 20;" text="Modifier" textFill="#f8f8f8" />
      <Button fx:id="delete" layoutX="362.0" layoutY="280.0" mnemonicParsing="false" onAction="#DeleteBakery" style="-fx-background-color: red; -fx-background-radius: 20;" text="Effacer" textFill="#f8f8f8" />
   </children>
</AnchorPane>
