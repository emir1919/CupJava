<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.cupcake.controllers.InfoBrandController">
   <children>
      <ImageView fitHeight="19.0" fitWidth="22.0" layoutX="355.0" layoutY="320.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/editar-h.png" />
         </image>
      </ImageView>
      <TableView fx:id="TableId" prefHeight="200.0" prefWidth="379.0">
        <columns>
          <TableColumn fx:id="nom" prefWidth="90.0" text="Nom" />
          <TableColumn fx:id="adresse" prefWidth="92.0" text="Adresse" />
            <TableColumn fx:id="tel" prefWidth="93.0" text="Tel" />
            <TableColumn fx:id="site" prefWidth="103.0" text="Site Web" />
        </columns>
      </TableView>
      <Button fx:id="edit" layoutX="147.0" layoutY="215.0" mnemonicParsing="false" onAction="#editbrand" prefHeight="25.0" prefWidth="85.0" style="-fx-background-color: blue; -fx-background-radius: 20;" text="Modifier" textFill="#f8f8f8">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
